.more {
  margin-top: 20rpx;
}

.confing__cell {
  padding: 20rpx 24rpx 20rpx 27rpx;
  position: relative;
  background-color: #fff;
  font-size: 28rpx;
  color: #333;
}

.confing__cell .cell__img {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

.confing__cell .bottom__line {
  position: absolute;
  height: 0.5rpx;
  bottom: 1rpx;
  left: 0;
  right: 0;
  background-color: #ddd;
  opacity: 0.5;
}

.confing__cell .arrow {
  width: 18rpx;
  height: 28rpx;
}

.confing__cell-content {
  flex: 1;
  text-align: right;
  padding: 0 10px;
}

.noList {
  text-align: center;
  margin-top: 100rpx;
  font-size: 38rpx;
  font-weight: bold;
  color: #888;
}

.tabber {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 12rpx;
}

.tabber-item {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
  padding: 10rpx;
  background-color: #fff;
  margin-bottom: 8rpx;
  box-sizing: border-box;
}

.tabber-item.picker {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 36rpx;
  height: 100%;
  margin: 0 8rpx;
  margin-bottom: 8rpx;
}

.tabber-item .checkbox {
  display: flex;
  align-items: center;
  vertical-align: middle;
}

.tabber-item .checkbox checkbox {
  margin-top: -4rpx;
}

.tabber-item.active {
  background-color: #9ed2ff;
  color: #fff;
}
.flex-1{
  flex: 1;
  overflow: hidden;
  box-sizing: border-box;
}

.list_box {
  margin-top: 24rpx;
}

.order_date {
  width: 180rpx;
  min-width: 180rpx;
  color: #555;
  overflow: hidden;
  box-sizing: border-box;
}

.total_count {
  font-weight: bold;
}

.productName{
  padding-right: 24rpx;
  overflow: hidden;
}